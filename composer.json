{
	"name": "lasserafn/php-hexer",
	"description": "A package to adjust brightness from a hex colorcode",
	"keywords": [
		"hex",
		"color",
		"brightness",
		"php"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		}
	],
	"require": {
		"php": "^5.6|^7.0|^7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\Hexer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Hexer\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
